<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shreerai.digitalcard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Welcome.WelcomeActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".SignInActivity"
            android:label="Digital Card"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TestActivityTwo" />
        <activity android:name=".SignUp.IntroActivity" />
        <activity android:name=".SignUp.EmailActivity" />
        <activity android:name=".SignUp.UsernameActivity" />
        <activity android:name=".SignUp.PasswordActivity" />
        <activity android:name=".SignUp.DetailActivity" />
        <activity android:name=".SignUp.CardSelectionActivity"></activity>
    </application>

</manifest>